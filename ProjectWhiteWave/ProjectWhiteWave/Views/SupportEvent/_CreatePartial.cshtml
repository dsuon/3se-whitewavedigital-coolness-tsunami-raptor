@model ProjectWhiteWave.Models.SupportEvent
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
@Html.AntiForgeryToken()

<h4>SupportEvent</h4>
<hr />

<div class="form-horizontal">

    @Html.ValidationSummary(true)

    <div class="form-group">
        @Html.LabelFor(model => model.DateOpened, new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.EditorFor(model => model.DateOpened)
            @Html.ValidationMessageFor(model => model.DateOpened)
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.DateClosed, new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.EditorFor(model => model.DateClosed)
            @Html.ValidationMessageFor(model => model.DateClosed)
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.Description, new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.EditorFor(model => model.Description)
            @Html.ValidationMessageFor(model => model.Description)
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Create" class="btn btn-default" />
        </div>
    </div>
</div>
</body>
</html>
